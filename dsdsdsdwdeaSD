entityTable.Debug.OnDeath = function()
    warn("Player has died.")
        local function GetGitSound(GithubSnd,SoundName)
					local url=GithubSnd
					if not isfile(SoundName..".mp3") then
						writefile(SoundName..".mp3", game:HttpGet(url))
					end
					local sound=Instance.new("Sound")
					sound.SoundId=(getcustomasset or getsynasset)(SoundName..".mp3")
					return sound
				end
						local AmbushJumpscareSound = GetGitSound("https://github.com/ZinXC/EntitiesTable/blob/main/y2mate.com%20-%20Doors%20Ambush%20jumpscare%20sound.mp3?raw=true","Ambushtrumsike") AmbushJumpscareSound.Parent = workspace
						AmbushJumpscareSound.Volume = 8
						AmbushJumpscareSound:Play()
						 
end
