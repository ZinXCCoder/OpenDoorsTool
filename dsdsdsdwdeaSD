        Min = 4,
        Max = 4,
        WaitTime = 2,
    },
    CamShake = {
        true, -- Enabled/Disabled
        {3.5, 20, 0.1, 1}, -- Shake values (don't change if you don't know)
        100, -- Shake start distance (from Entity to you)
    },
    Jumpscare = {
        true, -- Enabled/Disabled
        {
            Image1 = "", -- Image1 url
            Image2 = "https://tr.rbxcdn.com/38fe7e245fc87a6e2fb9cb490d24938d/420/420/Decal/Png", -- Image2 url
